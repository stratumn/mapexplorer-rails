{
  "name": "stratumn-sdk",
  "version": "1.5.0",
  "description": "Stratumn SDK for Javascript",
  "main": "lib/stratumn-sdk.js",
  "jsnext:main": "lib/stratumn-sdk.mjs",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive --require babel-polyfill --require should 2> /dev/null",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive --require should --require should 2> /dev/null",
    "lint": "eslint src test",
    "check": "npm run lint && npm run test",
    "build:lib": "rollup -c rollup.es.config.js && rollup -c rollup.umd.config.js",
    "build:bower": "rollup -c rollup.bower.config.js",
    "build:bower:min": "rollup -c rollup.bower.min.config.js",
    "build:all": "npm run build:lib && npm run build:bower && npm run build:bower:min",
    "clean": "rimraf lib coverage",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build:all",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build:all",
    "postpublish": "aws s3 cp dist/stratumn-sdk.js s3://stratumn-libs/stratumn-sdk.js --region 'eu-west-1' && aws s3 cp dist/stratumn-sdk.min.js s3://stratumn-libs/stratumn-sdk.min.js --region 'eu-west-1' && aws s3 cp dist/stratumn-sdk.js s3://stratumn-libs/stratumn-sdk-${npm_package_version}.js --region 'eu-west-1' && aws s3 cp dist/stratumn-sdk.min.js s3://stratumn-libs/stratumn-sdk-${npm_package_version}.min.js --region 'eu-west-1'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stratumn/stratumn-sdk-js.git"
  },
  "keywords": [
    "stratumn",
    "sdk",
    "blockchain",
    "client"
  ],
  "author": "Stratumn Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stratumn/stratumn-sdk-js/issues"
  },
  "homepage": "https://github.com/stratumn/stratumn-sdk-js",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.11.6",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-pluginutils": "^1.5.1",
    "should": "^8.2.2",
    "stratumn-agent": "^0.12.3"
  },
  "dependencies": {
    "httpplease": "^0.16.4",
    "xmlhttprequest": "~1.7.0"
  }
}
